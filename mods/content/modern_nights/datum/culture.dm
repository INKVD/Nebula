/decl/cultural_info/culture/african
	name = "African"
	description = "Your family is, ethnically or culturally, from one of the several nations spread across the African regions. North, south, central, west or east, you may have come from any of these regions, including former colonies."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/arabic,
		/decl/language/human/french,
		/decl/language/human/spanish,
		/decl/language/human/portuguese,
		/decl/language/human/german,
		/decl/language/human/italian
	)

/decl/cultural_info/culture/westnortheuro
	name = "North or Western European"
	description = "Your family is, ethnically or culturally, from somewhere in Western or North Europe. Austria, Belgium, France, Germany, Liechtenstein, Luxembourg, Monaco, the Netherlands or Switzerland are all valid regions for Westerners, while the United Kingdom, Denmark, Sweden, Iceland and Ireland are all valid examples for Northerners."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/french,
		/decl/language/human/german
	)

/decl/cultural_info/culture/southeuro
	name = "Mediterranean European"
	description = "Your family is, ethnically or culturally, from somewhere in whats normally called Southern Europe, which include any region in touch with the Mediterranean sea and the Iberic peninsula. The Balkans, Spain, Italy, Portugal, Greece, Turkey and more are all valid."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/portuguese,
		/decl/language/human/italian,
		/decl/language/human/spanish
	)

/decl/cultural_info/culture/easteuro
	name = "Eastern European"
	description = "Your family is, ethnically or culturally, from somewhere in Eastern Europe, which includes the Caucasus states, the Baltic states, Post-Soviet states, Central Europe and Southeastern Europe. Poland, Romania, Russia, Ukraine and Georgia are all valid examples."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/polish,
		/decl/language/human/russian
	)

/decl/cultural_info/culture/southasia
	name = "Southern Asian"
	description = "Your family is, ethnically or culturally, from somewhere in South Asia. Afghanistan, Bangladesh, Bhutan, India, Maldives, Nepal, Pakistan, and Sri Lanka are all valid examples."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/chinese,
		/decl/language/human/hindi
	)

/decl/cultural_info/culture/asia
	name = "Asian"
	description = "Your family is, ethnically or culturally, from somewhere in Central, Eastern or Northeastern Asia. China, mainland or not, Japan, Korea, Macau, Hong Kong and Borneo are all valid examples."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/chinese,
		/decl/language/human/russian
	)

/decl/cultural_info/culture/westasia
	name = "Western Asian"
	description = "Your family is, ethnically or culturally, from somewhere in Western Asia, or as its more commonly known, The Middle East, a geopolitical region encompassing Arabia, Asia Minor, East Thrace, Egypt, Iran, the Levant, Mesopotamia, and the Socotra Archipelago"
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/chinese,
		/decl/language/human/arabic,
		/decl/language/human/hindi,
		/decl/language/human/italian,
		/decl/language/human/french
	)

/decl/cultural_info/culture/oceania
	name = "Oceanian"
	description = "Your family is, ethnically or culturally, from somewhere in the Oceania region, which includes anything in Australia, New Zealand, Melanesia, Micronesia and Polynesia."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/chinese,
		/decl/language/human/french
	)

/decl/cultural_info/culture/na
	name = "North American"
	description = "Your family is, ethnically or culturally, from somewhere in North, or Central, America, be it in one of the numerous states of the United States, or Canada, or any of the other regions such as Cuba, the Carribean, Mexico or Bermuda, among others. "
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/french,
		/decl/language/human/spanish
	)

/decl/cultural_info/culture/sa
	name = "South American"
	description = "Your family is, ethnically or culturally, from somewhere in South America.  Argentina, Bolivia, Brazil, Chile or Colombia are valid examples, among many others."
	language = /decl/language/human/english
	secondary_langs = list(
		/decl/language/human/portuguese,
		/decl/language/human/spanish,
		/decl/language/human/french
	)
